      <Layer>
         <ows:Title xml:lang="en">OnEarth MVT Tile Test</ows:Title>
         <ows:WGS84BoundingBox crs="urn:ogc:def:crs:OGC:2:84">
            <ows:LowerCorner>-180 -90</ows:LowerCorner>
            <ows:UpperCorner>180 90</ows:UpperCorner>
        </ows:WGS84BoundingBox>
         <ows:Identifier>mvt_test</ows:Identifier>
         <Style isDefault="true">
             <ows:Title xml:lang="en">default</ows:Title>
             <ows:Identifier>default</ows:Identifier>
         </Style>
         <Format>application/vnd.mapbox-vector-tile</Format>
         <Dimension>
             <ows:Identifier>Time</ows:Identifier>
             <UOM>ISO8601</UOM>
             <Default>2012-01-01</Default>
             <Current>false</Current>
             <Value>2012-01-01/2012-01-01/P1D</Value>
         </Dimension>
         <TileMatrixSetLink>
             <TileMatrixSet>EPSG4326_16km</TileMatrixSet>
         </TileMatrixSetLink>
         <ResourceURL format="application/vnd.mapbox-vector-tile" resourceType="tile" template="http://localhost/onearth/test/wmts/mvt_test/default/{Time}/{TileMatrixSet}/{TileMatrix}/{TileRow}/{TileCol}.mvt"/>
      </Layer>
